<!--Menu To Back Animation-->
<animated-vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:drawable="@drawable/ic_menu_vector"
    tools:ignore="NewApi">
    <target
        android:name="menu"
        android:animation="@animator/menu_to_back_morph"/>
    <target
        android:name="rotation"
        android:animation="@animator/menu_to_back_rotation"/>
</animated-vector>